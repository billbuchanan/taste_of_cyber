			function getProdUrl(hrefUrl) {
			    var regexParse = new RegExp('[a-z\-0-9]{2,63}\.[a-z\.]{2,5}$');
			    var host = window.location.hostname;
			    var urlParts = regexParse.exec(host);
			    var env = host.replace(urlParts[0],'').slice(0, -1);
				var subenv = env.substring(0,6);

				//dev, stage, pre-prod
			    if(subenv === 'hotrod') {
			        host = ('www.hotrod.com');
			    }
			    if(typeof hrefUrl === "undefined" || hrefUrl === '') {
			        hrefUrl = window.location.href;
			    }
			    return hrefUrl.replace(window.location.hostname, host);
			}

			var currentUrl = encodeURIComponent(getProdUrl());

